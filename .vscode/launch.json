{
  "configurations": [
    {
      "name": "Test run: Barfus",
      "type": "cppdbg",
      "request": "launch",
      // "program": "${fileDirname}/${fileBasenameNoExtension}",
      // "program": "/usr/bin/gdb",
      // "args": [
      //   "-ex",
      //   "run",
      //   "--args",
      //   "${workspaceFolder}/build/src/KeysTracker",
      "program": "${workspaceFolder}/build/src/KeysTracker",
      "args": [
        "--input-video-filename=./barfus.h264",
        "--left-hand-white-key-color=hBC5763",
        "--left-hand-black-key-color=hBC5763",
        "--right-hand-white-key-color=h88C356",
        "--right-hand-black-key-color=h5C9B2A",
        "--first-octave-at=50",
        "--octaves-length=1204",
        "--number-of-octaves=7",
        // "--first-octave-positions=55,71,86,101,115,129,144,158,171,186,200,214",
        "--raw-frame-lines-to-extract=1",
        "--raw-frame-copy-from-line=616",
        "--track-mode=keys",
        "--number-of-frames-to-skip=5",
        "--out-filename=barfus.mid"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      // "preLaunchTask": "cppbuild",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      // "preLaunchTask": "make",

      "miDebuggerPath": "/usr/bin/gdb",

      "miDebuggerArgs": "-q -ex quit; wait() { fg >/dev/null; }; /bin/gdb -q --interpreter=mi"
    },
